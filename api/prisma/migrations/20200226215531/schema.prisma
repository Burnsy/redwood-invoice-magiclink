datasource sqlite {
  url = "***"
  provider = "sqlite"
}

generator photonjs {
  provider = "prisma-client-js"
}

model Invoice {
  id            String   @default(cuid()) @id
  invoiceNumber String
  date          String
  body          String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}