{
  "private": true,
  "name": "api",
  "version": "1.0.0",
  "dependencies": {
    "@hammerframework/hammer-api": "0.0.1-alpha.8",
    "@hammerframework/hammer-core": "^0.0.1-alpha.8",
    "@generated/photon": "0.0.0",
    "apollo-server-lambda": "^2.9.3",
    "dotenv": "^8.0.0",
    "graphql": "^14.5.6",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.5.1",
    "nexus": "^0.12.0-beta.8",
    "prisma2": "^2.0.0-preview-12"
  },
  "devDependencies": {
    "@hammerframework/hammer-dev-server": "^0.0.1-alpha.8.1",
    "@netlify/zip-it-and-ship-it": "https://github.com/netlify/zip-it-and-ship-it.git#feature/allow-parent-dirs"
  },
  "scripts": {
    "dev": "yarn hammer-dev-server",
    "db:up": "./scripts/prisma-default-env-vars.sh lift up && ./scripts/prisma-default-env-vars.sh generate",
    "db:save": "./scripts/prisma-default-env-vars.sh lift save",
    "build": "yarn clean && NODE_ENV=production yarn db:up && NODE_ENV=production babel src --out-dir dist",
    "package": "node ./scripts/package-functions.js",
    "clean": "rm -rf dist"
  }
}
